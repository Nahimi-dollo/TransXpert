<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="463dp"
        android:layout_height="862dp"
        android:src="@drawable/top_dashboard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView4"
        android:layout_width="150dp"
        android:layout_height="105dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/moi"
        app:civ_border_color="#FFFFFF"
        app:civ_border_width="@dimen/cardview_default_elevation"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.285" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="297dp"
        android:layout_height="29dp"
        android:layout_marginBottom="52dp"
        android:text="Bahani vissiley Thierry"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="331dp"
        android:layout_height="52dp"
        android:layout_marginBottom="488dp"
        android:text="Directeur générale"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:text="Transformateurs par region"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.331" />

    <TableLayout
        android:layout_width="335dp"
        android:layout_height="152dp"
        android:elevation="@dimen/cardview_default_elevation"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="@string/profil" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="Tabel" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="Tabel" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="Tabel" />
    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>